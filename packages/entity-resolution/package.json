    "./similarity": {
      "types": "./dist/similarity/index.d.ts",
      "import": "./dist/similarity/index.js"
    },
    "./blocking": {
      "types": "./dist/blocking/index.d.ts",
      "import": "./dist/blocking/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aliuyar1234/datatrust.git",
    "directory": "packages/entity-resolution"
  },
  "homepage": "https://github.com/aliuyar1234/datatrust",
